
@{
    ViewBag.Title = "Queue";
    Layout = "~/Views/Shared/MyLayout.cshtml";

    ViewBag.MenuItems += "<a href=" + @Url.Action("Index", "Index") + " class=\"w3-bar-item w3-button\">Return to Main Menu</a>";
}
<div class="w3-bar w3-light-gray my-subnavbar">
    <a href="@Url.Action("AddOne", "Queue")" class="w3-bar-item w3-button">Add One Item to Queue</a>
    <a href="@Url.Action("AddHugeList", "Queue")" class="w3-bar-item w3-button">Add Huge List of Items to Queue</a>
    <a href="@Url.Action("Display", "Queue")" class="w3-bar-item w3-button">Display Queue</a>
    <a href="@Url.Action("Clear", "Queue")" class="w3-bar-item w3-button">Delete From Queue</a>
    <a href="@Url.Action("Clear", "Queue")" class="w3-bar-item w3-button">Clear Queue</a>
    <a href="@Url.Action("Search", "Queue")" class="w3-bar-item w3-button">Search Queue</a>
</div>
<div class="alert-div">
    @Html.Raw((String)ViewBag.Error)
</div>
@Html.Raw((String)ViewBag.Search)
<br />
<div class="table-div">
    @Html.Raw((String)ViewBag.Queue)
</div>

