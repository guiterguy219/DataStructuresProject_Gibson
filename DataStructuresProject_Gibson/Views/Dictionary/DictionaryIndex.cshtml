
@{
    ViewBag.Title = "Dictionary";
    Layout = "~/Views/Shared/MyLayout.cshtml";

    ViewBag.MenuItems += "<a href=" + @Url.Action("Index", "Home") + " class=\"w3-bar-item w3-button\">Return to Main Menu</a>";
}
<div class="w3-bar w3-light-gray my-subnavbar">
    <a href="@Url.Action("AddOne", "Dictionary")" class="w3-bar-item w3-button">Add One Item to Dictionary</a>
    <a href="@Url.Action("AddHugeList", "Dictionary")" class="w3-bar-item w3-button">Add Huge List of Items to Dictionary</a>
    <a href="@Url.Action("Display", "Dictionary")" class="w3-bar-item w3-button">Display Dictionary</a>
    <a href="@Url.Action("Delete", "Dictionary")" class="w3-bar-item w3-button">Delete from Dictionary</a>
    <a href="@Url.Action("Clear", "Dictionary")" class="w3-bar-item w3-button">Clear Dictionary</a>
    <a href="@Url.Action("Search", "Dictionary")" class="w3-bar-item w3-button">Search Dictionary</a>
</div>
<div class="alert-div">
    @Html.Raw((String)ViewBag.Error)
</div>
@Html.Raw((String)ViewBag.Search)
<br />
<div class="table-div">
    @Html.Raw((String)ViewBag.Dictionary)
</div>

